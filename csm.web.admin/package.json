{
    "name": "admin-app",
    "version": "1.0.0",
    "description": "Admin Web-UI for Smart Site Project",
    "scripts": {
        "clean": "rm ./node_modules -rf",
        "serve": "npm run serve:dev --open",
        "serve:dev": "ng serve --configuration=dev --open --host 0.0.0.0",
        "serve:local": "ng serve --configuration=local --open --host 0.0.0.0",
        "serve:sandbox1": "ng serve --configuration=sandbox1 --open --host 0.0.0.0",
        "serve:sandbox2": "ng serve --configuration=sandbox2 --open --host 0.0.0.0",
        "serve:sandbox3": "ng serve --configuration=sandbox3 --open --host 0.0.0.0",
        "serve:sandbox4": "ng serve --configuration=sandbox4 --open --host 0.0.0.0",
        "build": "ng build",
        "prod": "ng build --configuration production --progress false",
        "test": "ng test",
        "test-coverage": "ng test --code-coverage",
        "lint": "eslint --ext .ts -o eslint-report.json --format json src; exit 0",
        "e2e": "ng e2e",
        "docs": "./node_modules/.bin/compodoc -p tsconfig.json -d docs",
        "static-analysis": "node scripts/static-analysis.js",
        "docker": "docker image build -t ptcsmacr.azurecr.io/com.bosch.pt/csm.web.admin ."
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "16.2.3",
        "@angular/cdk": "16.2.2",
        "@angular/common": "16.2.3",
        "@angular/compiler": "16.2.3",
        "@angular/core": "16.2.3",
        "@angular/forms": "16.2.3",
        "@angular/localize": "16.2.3",
        "@angular/material": "16.2.2",
        "@angular/platform-browser": "16.2.3",
        "@angular/platform-browser-dynamic": "16.2.3",
        "@angular/router": "16.2.3",
        "@ngrx/effects": "16.2.0",
        "@ngrx/store": "16.2.0",
        "@ngrx/store-devtools": "16.2.0",
        "@ngx-translate/core": "15.0.0",
        "@ngx-translate/http-loader": "8.0.0",
        "lodash": "4.17.21",
        "moment": "2.29.4",
        "rxjs": "6.6.3",
        "tslib": "2.6.2",
        "zone.js": "0.13.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "16.2.1",
        "@angular-eslint/builder": "16.1.1",
        "@angular-eslint/eslint-plugin": "16.1.1",
        "@angular-eslint/eslint-plugin-template": "16.1.1",
        "@angular-eslint/schematics": "16.1.1",
        "@angular-eslint/template-parser": "16.1.1",
        "@angular/cli": "16.2.1",
        "@angular/compiler-cli": "16.2.3",
        "@types/jasmine": "3.6.0",
        "@types/jasminewd2": "2.0.3",
        "@types/lodash": "4.14.168",
        "@types/node": "12.11.1",
        "@typescript-eslint/eslint-plugin": "5.59.2",
        "@typescript-eslint/parser": "5.59.2",
        "eslint": "8.39.0",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import": "2.27.4",
        "eslint-plugin-jsdoc": "39.6.4",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "jasmine-core": "3.6.0",
        "jasmine-spec-reporter": "5.0.2",
        "karma": "6.3.16",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage": "2.2.0",
        "karma-jasmine": "4.0.0",
        "karma-jasmine-html-reporter": "1.5.4",
        "properties": "1.2.1",
        "protractor": "7.0.0",
        "sonarqube-scanner": "2.8.0",
        "ts-node": "8.3.0",
        "typescript": "5.1.6"
    }
}
