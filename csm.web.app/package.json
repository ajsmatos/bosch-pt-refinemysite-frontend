{
    "name": "smartsite-web-app",
    "version": "1.0.0",
    "description": "Smartsite WebApp",
    "scripts": {
        "ng": "ng",
        "build": "npm install && npm run prod && npm run test-coverage",
        "serve": "npm run serve:dev --open",
        "serve:dev": "ng serve --configuration=dev --open --host 0.0.0.0",
        "serve:local": "ng serve --configuration=local --open --host 0.0.0.0",
        "serve:sandbox1": "ng serve --configuration=sandbox1 --open",
        "serve:sandbox2": "ng serve --configuration=sandbox2 --open",
        "serve:sandbox3": "ng serve --configuration=sandbox3 --open",
        "serve:sandbox4": "ng serve --configuration=sandbox4 --open",
        "prod": "ng build --configuration production --configuration=production",
        "local": "ng build --configuration production --configuration=local",
        "test": "ng test",
        "test-coverage": "ng test --code-coverage",
        "lint": "eslint --ext .ts -o eslint-report.json --format json src; exit 0",
        "clean": "rm -rf ./node_modules",
        "static-analysis": "node scripts/static-analysis.js",
        "check-licenses": "node scripts/check-licenses.js",
        "docker": "docker image build -t ptcsmacr.azurecr.io/com.bosch.pt/csm.web.app .",
        "prepare": "husky install"
    },
    "dependencies": {
        "@angular/animations": "16.2.2",
