        "@ngrx/store": "16.2.0",
        "@ngrx/store-devtools": "16.2.0",
        "@ngx-translate/core": "15.0.0",
        "@ngx-translate/http-loader": "8.0.0",
        "@swimlane/ngx-charts": "20.4.1",
        "bootstrap": "4.6.0",
        "core-js": "3.9.1",
